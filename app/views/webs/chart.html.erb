<script>
var dataset = [
	[   5,   150 ],
	[ 480,   300 ],
	[ 250,   400 ],
	[ 100,   330 ],
	[ 330,   50 ],
	[ 410,   120 ],
	[ 475,   340 ],
	[  25,   470 ],
	[  85,   210 ],
	[ 220,   380 ]];
 
var svg = d3.select("body")
		.append("svg")
		.attr("width", 1000)
		.attr("height", 1000);
 
svg.selectAll("circle")
	.data(dataset)
	.enter()
	.append("circle")
	.attr({
		cx : function(d) {return d[0] + 50},
		cy : function(d) {return 500 - d[1]},
		r : function(d) {return 10},
		fill : "red"
	})
 
</script>